{
  "cli": {
    "version": ">= 13.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://foodsystemsanalytics.com/api",
        "EXPO_PUBLIC_ENVIRONMENT": "production"
      },
      "android": {
        "buildType": "apk"
      },
      "node": "22.11.0"
    }
  },
  "submit": {
    "production": {}
  }
}
