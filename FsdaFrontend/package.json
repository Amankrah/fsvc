{
  "name": "fsdafrontend",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "start:dev": "EXPO_PUBLIC_ENVIRONMENT=development expo start",
    "start:prod": "EXPO_PUBLIC_ENVIRONMENT=production expo start --no-dev",
    "android": "expo run:android",
    "android:dev": "EXPO_PUBLIC_ENVIRONMENT=development expo start --android",
    "ios": "expo run:ios",
    "ios:dev": "EXPO_PUBLIC_ENVIRONMENT=development expo start --ios",
    "web": "expo start --web",
    "web:dev": "EXPO_PUBLIC_ENVIRONMENT=development expo start --web",
    "web:prod": "EXPO_PUBLIC_ENVIRONMENT=production expo start --web --no-dev",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "build:web": "expo export --platform web --output-dir web-build",
    "build:web:prod": "EXPO_PUBLIC_ENVIRONMENT=production expo export --platform web --output-dir web-build",
    "deploy:web": "bash deploy-web.sh",
    "clean": "expo start -c"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "axios": "^1.12.2",
    "expo": "~54.0.11",
    "expo-clipboard": "^8.0.7",
    "expo-constants": "~18.0.11",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.20",
    "expo-image-picker": "^17.0.8",
    "expo-location": "^19.0.7",
    "expo-secure-store": "~15.0.7",
    "expo-sharing": "~14.0.8",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "react-native": "0.81.5",
    "react-native-paper": "^5.14.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@react-native/eslint-config": "^0.81.1",
    "@types/react": "~19.1.10",
    "@types/react-native": "^0.72.8",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  },
  "private": true
}
